{
    "type": "node",
    "request": "launch",
    "name": "Launch Program",
    "protocol": "inspector",
    "runtimeExecutable": "nodemon",
    "runtimeArgs": [
        "--inspect=9222"
    ],
    "program": "${workspaceFolder}/index.js",
    "port": 9222,
    "restart": true,
    "env": {
        "BABEL_DISABLE_CACHE": "1",
        "NODE_ENV": "development"
    },
    "console": "integratedTerminal",
    "internalConsoleOptions": "neverOpen",
    "configurations": [
    {
        "command": "npm start",
        "name": "Run npm start",
        "request": "launch",
        "type": "node-terminal"
    }
    ]
}